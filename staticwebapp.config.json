{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/static/*", "/*.{js,css,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.{js,css}",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    }
  ],
  "mimeTypes": {
    ".json": "application/json"
  }
}
